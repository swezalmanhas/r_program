1.
sol<-dbinom(7,12,1/6)+dbinom(8,12,1/6)+dbinom(9,12,1/6)
print(sol)

sol<-pbinom(9,12,1/6)-pbinom(6,12,1/6)
print(sol)

2.
sol=pnorm(84,mean=72,sd=15.2,lower.tail = FALSE)
print(sol)

3.
sol<-ppois(50,50)-ppois(47,50)
print(sol)

q=dpois(0,5)
print(q)

4.
sol=dhyper(3,m=17,n=233,k=5)
print(sol)

5.

x<-seq(0,31)
print(x)
pmf<-dbinom(x,31,0.447)
plot(pmf)

cmf<-pbinom(x,31,0.447)
plot(cmf)
mean(x)
sd(x)
var(x)
